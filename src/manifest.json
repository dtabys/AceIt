{
	"manifest_version": 2,
	"name": "AceIT",
	"version": "1.0",
	"description": "Unblurs Chegg homework solutions.",
	"icons": {
		"16": "images/a_plus16.png",
		"32": "images/a_plus32.png",
		"48": "images/a_plus48.png",
		"128": "images/a_plus128.png"
	},
	"content_scripts": [{
		"js": ["unobfuscation.js"],
		"matches": ["https://www.chegg.com/homework-help/*"]
	}],
	"permissions": [
	  "https://textsheet.com/"
	]
}